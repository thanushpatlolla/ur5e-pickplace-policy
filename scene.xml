<mujoco model="ur5e scene">
  <include file="ur5e.xml"/>

  <compiler angle="radian"/>

  <statistic center="0.4 0 0.5" extent="1.2" meansize="0.08"/>

  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0.3 0.3 0.3"/>
    <global azimuth="150" elevation="-25"/>
  </visual>

  <asset>
    <texture name="grid" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" width="512" height="512"
      mark="cross" markrgb=".8 .8 .8"/>
    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true"/>
    <material name="table" rgba="0.6 0.4 0.2 1"/>
    <material name="object" rgba="0.9 0.2 0.2 1"/>

    <!-- Robotiq 2F-85 Gripper Assets -->
    <material name="metal" rgba="0.58 0.58 0.58 1"/>
    <material name="silicone" rgba="0.1882 0.1882 0.1882 1"/>
    <material name="gray" rgba="0.4627 0.4627 0.4627 1"/>
    <material name="robotiq_black" rgba="0.149 0.149 0.149 1"/>

    <mesh file="base_mount.stl" scale="0.001 0.001 0.001"/>
    <mesh file="base.stl" scale="0.001 0.001 0.001"/>
    <mesh file="driver.stl" scale="0.001 0.001 0.001"/>
    <mesh file="coupler.stl" scale="0.001 0.001 0.001"/>
    <mesh file="follower.stl" scale="0.001 0.001 0.001"/>
    <mesh file="pad.stl" scale="0.001 0.001 0.001"/>
    <mesh file="silicone_pad.stl" scale="0.001 0.001 0.001"/>
    <mesh file="spring_link.stl" scale="0.001 0.001 0.001"/>
  </asset>

  <worldbody>
    <light pos="0.5 0.5 2.0" dir="0 0 -1" directional="false"/>
    <light pos="0.5 -0.5 2.0" dir="0 0 -1" directional="false"/>

    <geom name="floor" size="2 2 0.01" type="plane" material="grid" pos="0 0 0"/>

    <!-- Table at height 0.4m, dimensions 0.8m x 1.0m x 0.05m -->
    <body name="table" pos="0.4 0 0.2">
      <geom name="table_top" type="box" size="0.4 0.5 0.025" pos="0 0 0.175" material="table" mass="10"/>
      <geom name="table_leg1" type="cylinder" size="0.03 0.175" pos="0.3 0.4 0" rgba="0.4 0.3 0.15 1"/>
      <geom name="table_leg2" type="cylinder" size="0.03 0.175" pos="0.3 -0.4 0" rgba="0.4 0.3 0.15 1"/>
      <geom name="table_leg3" type="cylinder" size="0.03 0.175" pos="-0.3 0.4 0" rgba="0.4 0.3 0.15 1"/>
      <geom name="table_leg4" type="cylinder" size="0.03 0.175" pos="-0.3 -0.4 0" rgba="0.4 0.3 0.15 1"/>
    </body>

    <!-- Robot base positioned at edge of table -->
    <body name="robot_mount" pos="0 0 0.4">
      <body name="base" quat="1 0 0 1" childclass="ur5e">
        <inertial mass="4" pos="0 0 0" diaginertia="0.00443333156 0.00443333156 0.0072"/>
        <geom mesh="base_0" material="black" class="visual"/>
        <geom mesh="base_1" material="jointgray" class="visual"/>
        <body name="shoulder_link" pos="0 0 0.163">
          <inertial mass="3.7" pos="0 0 0" diaginertia="0.0102675 0.0102675 0.00666"/>
          <joint name="shoulder_pan" axis="0 0 1"/>
          <geom mesh="shoulder_0" material="urblue" class="visual"/>
          <geom mesh="shoulder_1" material="black" class="visual"/>
          <geom mesh="shoulder_2" material="jointgray" class="visual"/>
          <geom name="shoulder_collision" class="collision" size="0.06 0.06" pos="0 0 -0.04"/>
          <body name="upper_arm_link" pos="0 0.138 0" quat="1 0 1 0">
            <inertial mass="8.393" pos="0 0 0.2125" diaginertia="0.133886 0.133886 0.0151074"/>
            <joint name="shoulder_lift"/>
            <geom mesh="upperarm_0" material="linkgray" class="visual"/>
            <geom mesh="upperarm_1" material="black" class="visual"/>
            <geom mesh="upperarm_2" material="jointgray" class="visual"/>
            <geom mesh="upperarm_3" material="urblue" class="visual"/>
            <geom name="upper_arm_collision_1" class="collision" pos="0 -0.04 0" quat="1 1 0 0" size="0.06 0.06"/>
            <geom name="upper_arm_collision_2" class="collision" size="0.05 0.2" pos="0 0 0.2"/>
            <body name="forearm_link" pos="0 -0.131 0.425">
              <inertial mass="2.275" pos="0 0 0.196" diaginertia="0.0311796 0.0311796 0.004095"/>
              <joint name="elbow" class="joint_limited"/>
              <geom mesh="forearm_0" material="urblue" class="visual"/>
              <geom mesh="forearm_1" material="linkgray" class="visual"/>
              <geom mesh="forearm_2" material="black" class="visual"/>
              <geom mesh="forearm_3" material="jointgray" class="visual"/>
              <geom name="forearm_collision_1" class="collision" pos="0 0.08 0" quat="1 1 0 0" size="0.055 0.06"/>
              <geom name="forearm_collision_2" class="collision" size="0.038 0.19" pos="0 0 0.2"/>
              <body name="wrist_1_link" pos="0 0 0.392" quat="1 0 1 0">
                <inertial mass="1.219" pos="0 0.127 0" diaginertia="0.0025599 0.0025599 0.0021942"/>
                <joint name="wrist_1"/>
                <geom mesh="wrist1_0" material="black" class="visual"/>
                <geom mesh="wrist1_1" material="urblue" class="visual"/>
                <geom mesh="wrist1_2" material="jointgray" class="visual"/>
                <geom name="wrist_1_collision" class="collision" pos="0 0.05 0" quat="1 1 0 0" size="0.04 0.07"/>
                <body name="wrist_2_link" pos="0 0.127 0">
                  <inertial mass="1.219" pos="0 0 0.1" diaginertia="0.0025599 0.0025599 0.0021942"/>
                  <joint name="wrist_2" axis="0 0 1"/>
                  <geom mesh="wrist2_0" material="black" class="visual"/>
                  <geom mesh="wrist2_1" material="urblue" class="visual"/>
                  <geom mesh="wrist2_2" material="jointgray" class="visual"/>
                  <geom name="wrist_2_link" class="collision" size="0.04 0.06" pos="0 0 0.04"/>
                  <body name="wrist_3_link" pos="0 0 0.1">
                    <inertial mass="0.1879" pos="0 0.0771683 0" quat="1 0 0 1"
                      diaginertia="0.000132134 9.90863e-05 9.90863e-05"/>
                    <joint name="wrist_3"/>
                    <geom material="linkgray" mesh="wrist3" class="visual"/>
                    <geom name="wrist_3_link" class="collision" pos="0 0.025 0" quat="1 1 0 0" size="0.04 0.05"/>
                    <site name="attachment_site" size="0.01" pos="0 0.1 0" quat="-1 1 1 1" rgba="1 0 0 1" group="1"/>

                    <!-- Robotiq 2F-85 Gripper -->
                    <body name="gripper_base_mount" pos="0 0.1 0" quat="0.707107 0 0 0.707107">
                      <geom type="mesh" mesh="base_mount" material="robotiq_black" contype="0" conaffinity="0"/>
                      <geom type="mesh" mesh="base_mount"/>
                      <body name="gripper_base" pos="0 0 0.0038" quat="1 0 0 -1">
                        <inertial mass="0.777441" pos="0 -2.70394e-05 0.0354675" quat="1 -0.00152849 0 0"
                          diaginertia="0.000260285 0.000225381 0.000152708"/>
                        <geom type="mesh" mesh="base" material="robotiq_black" contype="0" conaffinity="0"/>
                        <geom type="mesh" mesh="base"/>
                        <site name="pinch" pos="0 0 0.145" type="sphere" group="5" rgba="0.9 0.9 0.9 1" size="0.005"/>
                        <site name="grasp_site" pos="0 0 0.145" size="0.01" rgba="0 1 0 0.3"/>

                        <!-- Right-hand side 4-bar linkage -->
                        <body name="right_driver" pos="0 0.0306011 0.054904">
                          <inertial mass="0.00899563" pos="2.96931e-12 0.0177547 0.00107314" quat="0.681301 0.732003 0 0"
                            diaginertia="1.72352e-06 1.60906e-06 3.22006e-07"/>
                          <joint name="right_driver_joint" axis="1 0 0" range="0 0.8" armature="0.005" damping="0.1"
                            solimplimit="0.95 0.99 0.001" solreflimit="0.005 1"/>
                          <geom type="mesh" mesh="driver" material="gray" contype="0" conaffinity="0"/>
                          <geom type="mesh" mesh="driver"/>
                          <body name="right_coupler" pos="0 0.0315 -0.0041">
                            <inertial mass="0.0140974" pos="0 0.00301209 0.0232175" quat="0.705636 -0.0455904 0.0455904 0.705636"
                              diaginertia="4.16206e-06 3.52216e-06 8.88131e-07"/>
                            <joint name="right_coupler_joint" axis="1 0 0" range="-1.57 0" armature="0.001"
                              solimplimit="0.95 0.99 0.001" solreflimit="0.005 1"/>
                            <geom type="mesh" mesh="coupler" material="robotiq_black" contype="0" conaffinity="0"/>
                            <geom type="mesh" mesh="coupler"/>
                          </body>
                        </body>

                        <body name="right_spring_link" pos="0 0.0132 0.0609">
                          <inertial mass="0.0221642" pos="-8.65005e-09 0.0181624 0.0212658" quat="0.663403 -0.244737 0.244737 0.663403"
                            diaginertia="8.96853e-06 6.71733e-06 2.63931e-06"/>
                          <joint name="right_spring_link_joint" axis="1 0 0" range="-0.29670597283 0.8" armature="0.001"
                            stiffness="0.05" springref="2.62" damping="0.00125"/>
                          <geom type="mesh" mesh="spring_link" material="robotiq_black" contype="0" conaffinity="0"/>
                          <geom type="mesh" mesh="spring_link"/>
                          <body name="right_follower" pos="0 0.055 0.0375">
                            <inertial mass="0.0125222" pos="0 -0.011046 0.0124786" quat="1 0.1664 0 0"
                              diaginertia="2.67415e-06 2.4559e-06 6.02031e-07"/>
                            <joint name="right_follower_joint" axis="1 0 0" range="-0.872664 0.872664" armature="0.001"
                              pos="0 -0.018 0.0065" solimplimit="0.95 0.99 0.001" solreflimit="0.005 1"/>
                            <geom type="mesh" mesh="follower" material="robotiq_black" contype="0" conaffinity="0"/>
                            <geom type="mesh" mesh="follower"/>
                            <body name="right_pad" pos="0 -0.0189 0.01352">
                              <geom type="box" name="right_pad1" mass="0" pos="0 -0.0026 0.028125" size="0.011 0.004 0.009375"
                                friction="0.7" solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" rgba="0.55 0.55 0.55 1"/>
                              <geom type="box" name="right_pad2" mass="0" pos="0 -0.0026 0.009375" size="0.011 0.004 0.009375"
                                friction="0.6" solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" rgba="0.45 0.45 0.45 1"/>
                              <inertial mass="0.0035" pos="0 -0.0025 0.0185" quat="0.707107 0 0 0.707107"
                                diaginertia="4.73958e-07 3.64583e-07 1.23958e-07"/>
                              <geom type="mesh" mesh="pad" contype="0" conaffinity="0"/>
                              <body name="right_silicone_pad">
                                <geom type="mesh" mesh="silicone_pad" material="robotiq_black" contype="0" conaffinity="0"/>
                              </body>
                            </body>
                          </body>
                        </body>

                        <!-- Left-hand side 4-bar linkage -->
                        <body name="left_driver" pos="0 -0.0306011 0.054904" quat="0 0 0 1">
                          <inertial mass="0.00899563" pos="0 0.0177547 0.00107314" quat="0.681301 0.732003 0 0"
                            diaginertia="1.72352e-06 1.60906e-06 3.22006e-07"/>
                          <joint name="left_driver_joint" axis="1 0 0" range="0 0.8" armature="0.005" damping="0.1"
                            solimplimit="0.95 0.99 0.001" solreflimit="0.005 1"/>
                          <geom type="mesh" mesh="driver" material="gray" contype="0" conaffinity="0"/>
                          <geom type="mesh" mesh="driver"/>
                          <body name="left_coupler" pos="0 0.0315 -0.0041">
                            <inertial mass="0.0140974" pos="0 0.00301209 0.0232175" quat="0.705636 -0.0455904 0.0455904 0.705636"
                              diaginertia="4.16206e-06 3.52216e-06 8.88131e-07"/>
                            <joint name="left_coupler_joint" axis="1 0 0" range="-1.57 0" armature="0.001"
                              solimplimit="0.95 0.99 0.001" solreflimit="0.005 1"/>
                            <geom type="mesh" mesh="coupler" material="robotiq_black" contype="0" conaffinity="0"/>
                            <geom type="mesh" mesh="coupler"/>
                          </body>
                        </body>

                        <body name="left_spring_link" pos="0 -0.0132 0.0609" quat="0 0 0 1">
                          <inertial mass="0.0221642" pos="-8.65005e-09 0.0181624 0.0212658" quat="0.663403 -0.244737 0.244737 0.663403"
                            diaginertia="8.96853e-06 6.71733e-06 2.63931e-06"/>
                          <joint name="left_spring_link_joint" axis="1 0 0" range="-0.29670597283 0.8" armature="0.001"
                            stiffness="0.05" springref="2.62" damping="0.00125"/>
                          <geom type="mesh" mesh="spring_link" material="robotiq_black" contype="0" conaffinity="0"/>
                          <geom type="mesh" mesh="spring_link"/>
                          <body name="left_follower" pos="0 0.055 0.0375">
                            <inertial mass="0.0125222" pos="0 -0.011046 0.0124786" quat="1 0.1664 0 0"
                              diaginertia="2.67415e-06 2.4559e-06 6.02031e-07"/>
                            <joint name="left_follower_joint" axis="1 0 0" range="-0.872664 0.872664" armature="0.001"
                              pos="0 -0.018 0.0065" solimplimit="0.95 0.99 0.001" solreflimit="0.005 1"/>
                            <geom type="mesh" mesh="follower" material="robotiq_black" contype="0" conaffinity="0"/>
                            <geom type="mesh" mesh="follower"/>
                            <body name="left_pad" pos="0 -0.0189 0.01352">
                              <geom type="box" name="left_pad1" mass="0" pos="0 -0.0026 0.028125" size="0.011 0.004 0.009375"
                                friction="0.7" solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" rgba="0.55 0.55 0.55 1"/>
                              <geom type="box" name="left_pad2" mass="0" pos="0 -0.0026 0.009375" size="0.011 0.004 0.009375"
                                friction="0.6" solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" rgba="0.45 0.45 0.45 1"/>
                              <inertial mass="0.0035" pos="0 -0.0025 0.0185" quat="1 0 0 1"
                                diaginertia="4.73958e-07 3.64583e-07 1.23958e-07"/>
                              <geom type="mesh" mesh="pad" contype="0" conaffinity="0"/>
                              <body name="left_silicone_pad">
                                <geom type="mesh" mesh="silicone_pad" material="robotiq_black" contype="0" conaffinity="0"/>
                              </body>
                            </body>
                          </body>
                        </body>
                      </body>
                    </body>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>

    <!-- Dynamic cuboid object (will be randomized in code) -->
    <body name="object" pos="0.4 0 0.45">
      <freejoint name="object"/>
      <inertial pos="0 0 0" mass="0.1" diaginertia="0.0001 0.0001 0.0001"/>
      <geom name="object_geom" type="box" size="0.03 0.04 0.025" material="object" friction="1.0 0.005 0.0001"/>
      <site name="object_site" pos="0 0 0" size="0.01" rgba="0 0 1 0.5"/>
    </body>
  </worldbody>

  <contact>
    <exclude body1="gripper_base" body2="right_driver"/>
    <exclude body1="gripper_base" body2="left_driver"/>
    <exclude body1="gripper_base" body2="right_spring_link"/>
    <exclude body1="gripper_base" body2="left_spring_link"/>
    <exclude body1="right_coupler" body2="right_follower"/>
    <exclude body1="left_coupler" body2="left_follower"/>
  </contact>

  <tendon>
    <fixed name="split">
      <joint joint="right_driver_joint" coef="0.5"/>
      <joint joint="left_driver_joint" coef="0.5"/>
    </fixed>
  </tendon>

  <equality>
    <!-- Robotiq gripper constraints -->
    <connect anchor="0 0 0" body1="right_follower" body2="right_coupler" solimp="0.95 0.99 0.001" solref="0.005 1"/>
    <connect anchor="0 0 0" body1="left_follower" body2="left_coupler" solimp="0.95 0.99 0.001" solref="0.005 1"/>
    <joint joint1="right_driver_joint" joint2="left_driver_joint" polycoef="0 1 0 0 0" solimp="0.95 0.99 0.001" solref="0.005 1"/>

    <!-- Weld constraint removed - using physical gripper grasping instead -->
    <!-- <weld name="grasp_weld" body1="gripper_base" body2="object" active="false" solref="0.01 1"/> -->
  </equality>

  <!-- Actuators are defined in ur5e.xml, plus gripper actuator below -->
  <actuator>
    <general name="fingers_actuator" tendon="split" forcerange="-5 5" ctrlrange="0 255"
      gainprm="0.3137255 0 0" biasprm="0 -100 -10" biastype="affine"/>
  </actuator>

  <keyframe>
    <key name="home" qpos="-1.5708 -1.5708 1.5708 -1.5708 -1.5708 0 0.4 0 0.45 1 0 0 0" ctrl="-1.5708 -1.5708 1.5708 -1.5708 -1.5708 0"/>
  </keyframe>
</mujoco>
